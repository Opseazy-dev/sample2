    <section>
        <div class="dashcontainer">
            <div class="addbtn">
                <a class="loginbutton" href="/adduser">Add User</a>
                <a href="/" class="loginbutton">Sign Out</a>
            </div>
            
                {{#each users}}
                <div class="navtab">
                    <div class="tableft">
                        <h3>{{username}}</h3>
                    </div>
                </div>                
                {{/each}}
                {{!-- <div class="tabright">
                    <a href="#">Edit</a>
                    <a href="#">Delete</a>
                </div> --}}
        </div>


    </section>
    
